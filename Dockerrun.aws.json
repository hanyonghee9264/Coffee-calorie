{
    "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
        {
            "environment": [
                {
                    "name": "HUB_PORT_4444_TCP_ADDR",
                    "value": "hub"
                },
                {
                    "name": "HUB_PORT_4444_TCP_PORT",
                    "value": "4444"
                }
            ],
            "essential": true,
            "image": "selenium/node-chrome",
            "name": "chrome",
            "portMappings": [
                {
                    "containerPort": 5900,
                    "hostPort": 5900
                }
            ]
        },
        {
            "environment": [
                {
                    "name": "POSTGRES_DB",
                    "value": "coffeecalorie"
                },
                {
                    "name": "POSTGRES_USER",
                    "value": "hanyonghee"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": "samplesecret"
                }
            ],
            "essential": true,
            "image": "postgres",
            "mountPoints": [
                {
                    "containerPath": "/var/lib/postgresql/data",
                    "sourceVolume": "Postgres_Data"
                }
            ],
            "name": "db"
        },
        {
            "command": [
                "bash",
                "-c",
                "python /srv/projects/app/manage.py migrate\nsupervisord -n\nredis-server\n"
            ],
            "environment": [
                {
                    "name": "DJANGO_DEBUG",
                    "value": "True"
                },
                {
                    "name": "DJANGO_DB_HOST",
                    "value": "postgres"
                },
                {
                    "name": "DJANGO_DB_PORT",
                    "value": "5432"
                },
                {
                    "name": "DJANGO_DB_NAME",
                    "value": "coffeecalorie"
                },
                {
                    "name": "DJANGO_DB_USERNAME",
                    "value": "hanyonghee"
                },
                {
                    "name": "DJANGO_DB_PASSWORD",
                    "value": "samplesecret"
                },
                {
                    "name": "DJANGO_SECRET_KEY",
                    "value": "dev_secret_key"
                }
            ],
            "essential": true,
            "links": [
                "db:postgres",
                "chrome",
                "hub"
            ],
            "name": "django",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 7000
                }
            ]
        },
        {
            "essential": true,
            "image": "selenium/hub",
            "name": "hub",
            "portMappings": [
                {
                    "containerPort": 4444,
                    "hostPort": 4444
                }
            ]
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "postgres_data"
            },
            "name": "Postgres_Data"
        }
    ]
}
